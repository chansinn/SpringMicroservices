package com.chansin.web.fiegn;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;

@FeignClient(name="ChansinWebProject")
public interface ChansinWebProxy {
	
	@RequestMapping(method=RequestMethod.GET, value="/getEmployeeDetails")
	Object getEmployees();
	
	@RequestMapping(value="/getSingleEmployee/{id}", method=RequestMethod.GET)
	Object getEmployeeById(@PathVariable("id") String id);
	
	

}
